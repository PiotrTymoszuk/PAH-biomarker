journal = {Clinical infectious diseases : an official publication of the Infectious Diseases Society of America},
keywords = {COVID-19,SARS-CoV-2,long-COVID,long-term sequelae,phenotyping},
month = {nov},
number = {1},
pages = {e418--e431},
pmid = {34849652},
title = {{Phenotyping of Acute and Persistent Coronavirus Disease 2019 Features in the Outpatient Setting: Exploratory Analysis of an International Cross-sectional Online Survey}},

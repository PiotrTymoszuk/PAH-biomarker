journal = {Chest},
keywords = {Adult,Aged,Female,Glomerular Filtration Rate,Harrison W Farber,Humans,Kaplan-Meier Estimate,MEDLINE,Male,Manreet K Kanwar,Middle Aged,NCBI,NIH,NLM,National Center for Biotechnology Information,National Institutes of Health,National Library of Medicine,Non-U.S. Gov't,PMC7462639,Physical Endurance,Proportional Hazards Models,PubMed Abstract,Pulmonary Arterial Hypertension / complications,Pulmonary Arterial Hypertension / mortality*,Pulmonary Arterial Hypertension / physiopathology*,Raymond L Benza,Reproducibility of Results,Research Support,Risk Assessment,Survival Rate,doi:10.1016/j.chest.2020.08.2069,pmid:32882243},
